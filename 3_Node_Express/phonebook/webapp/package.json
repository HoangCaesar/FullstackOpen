    "logs:prod": "heroku logs --tail"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "json-server": "^0.17.0",
    "vite": "^3.1.0"
  }
}
