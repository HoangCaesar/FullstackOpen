{
  "name": "phonebook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "proxy": "http://localhost:3001",
  "scripts": {
    "dev": "vite --port=3000",
    "build": "vite build",
    "preview": "vite preview",
    "build:ui": "rm -rf build && cd ../part2-notes/ && npm run build && cp -r build ../notes-backend",
